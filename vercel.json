{
    "version": 2,
    "builds": [
        {
            "src": "./app",
            "use": "@vercel/python"
            "src":"app.py",
            "use":"@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
            "src":"/(.*)", 
            "dest":"app.py"
        }
    ]
}
